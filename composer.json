{
    "name": "triage/triage",
    "description": "A utility which checks webapp sources for compatibility with microformats semantics",
    "type": "project",
    "require": {
        "sabberworm/php-css-parser": "^8.1",
        "carica/phpcss": "^1.0"
    },
    "require-dev": {
        "behat/behat": "^3.4",
        "phpspec/phpspec": "^4.3",
        "phpmd/phpmd": "^2.6",
        "theseer/phpdox": "^0.11.2",
        "phpunit/phpunit": "^6.5",
        "sebastian/phpcpd": "^3.0",
        "phploc/phploc": "^4.0",
        "phpstan/phpstan": "^0.9.2",
        "edgedesign/phpqa": "^1.19",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "jakub-onderka/php-console-highlighter": "^0.3.2",
        "pdepend/pdepend": "^2.5",
        "squizlabs/php_codesniffer": "^3.3",
        "phpmetrics/phpmetrics": "^2.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher James Willcock",
            "email": "cjwillcock@ieee.org"
        }
    ],
    "autoload": {
    	"psr-4": {
		"Triage\\": "app/code"
	},
	"files": [
		"app/utilities.php"
	]
    },
    "extra": {
    	"branch-alias": {
		"dev-master": "0.1.x-dev"
	}
    },
    "bin": [
    	"bin/triage"
    ],
    "scripts": {
        "post-update-cmd": [
	   "bin/latest-mime-types",
	   "bin/latest-microformats"
	]
    }
}
